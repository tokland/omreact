{
  "name": "omreact",
  "version": "0.0.3",
  "description": "Purely functional React components with local state",
  "license": "MIT",
  "repository": "tokland/omreact",
  "author": {
    "name": "Arnau Sanchez",
    "url": "https://stackoverflow.com/users/188031/tokland"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "index.js",
    "component.js",
    "commands.js"
  ],
  "scripts": {
    "_precommit": "lint-staged",
    "lint": "eslint 'src/**/*.js' 'test/**/*.js'",
    "code-quality": "npm run lint",
    "build": "babel src -d . --source-maps --ignore 'src/**/*.test.js'",
    "build-watch": "babel src -d . --source-maps --ignore 'src/**/*.test.js' --watch",
    "prepublish": "npm run build",
    "release": "npm run code-quality && npm run build && npm publish"
  },
  "main": "index.js",
  "keywords": [
    "react",
    "functional",
    "state",
    "reducer",
    "immutable",
    "elm"
  ],
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.1",
    "lodash": "^4.17.10",
    "memoize-weak": "^1.0.2",
    "prop-types": "^15.6.2",
    "shouldcomponentupdate-children": "^1.1.0"
  },
  "peerDependencies": {
    "react": "^15.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "8",
    "babel-jest": "^23.4.2",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "4.x",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "jest": "^23.5.0",
    "lint-staged": "^7.0.4",
    "prettier": "^1.11.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "regenerator-runtime": "^0.12.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/config/setupTests.js",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
